
@{
    ViewBag.Title = "ConfirmByOnlineBanking";
}


<div>
    <br />
    <img class="img-responsive" src="~/Asset/images/Progress4.png" />
</div>

@*Content*@
<div>
    <h2>ชำระด้วยบริการ Internet Banking</h2>

    <form class="form-horizontal">
        <fieldset class="well">
            @*ชื่อธนาคารผู้ออกบัตร*@
            <div class="form-group">
                <label class="col-md-3 control-label">ชื่อธนาคารผู้ออกบัตร</label>
                <div class="col-md-7">
                    <select class="form-control">
                        <option>กสิกร</option>
                        <option>ไทยพาณิชย์</option>
                        <option>กรุงไทย</option>
                    </select>
                </div>
            </div>
            <br />
        </fieldset>
    </form>


    @*ถัดไป*@
    <div class="row">
        <a class="btn btn-lg btn-success pull-left" href="/Register/Payment"><span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span> ย้อนกลับ</a>
        <a class="btn btn-lg btn-success pull-right" href="/Register/PrintExanCard">ยืนยันการชำระเงิน <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span></a>
    </div>
</div>
